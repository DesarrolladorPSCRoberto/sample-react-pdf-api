{
  "name": "sample",
  "version": "1.0.0",
  "description": "Example project to showcase rendering pdfs on the backend with React-PDF",
  "main": "index.ts",
  "scripts": {
    "build": "tsc --build",
    "serve:build": "node build/index.js",
    "start:dev": "cross-env NODE_ENV=development nodemon src/index.ts",
    "start:test": "cross-env NODE_ENV=test nodemon src/index.ts",
    "start:stage": "cross-env NODE_ENV=staging nodemon src/index.ts",
    "start:prod": "npm run build && npm run serve:build"
  },
  "author": "Export SDK",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^18.19.127",
    "cross-env": "^10.0.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@react-pdf/renderer": "^2.0.21",
    "dotenv": "^17.2.2",
    "env": "^0.0.2",
    "express": "^4.17.2",
    "react": "^17.0.2"
  }
}